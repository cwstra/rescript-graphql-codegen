{
  "name": "rescript-graphql-codegen",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "esbuild": "^0.20.2",
    "graphql": "^16.0.0"
  },
  "dependencies": {
    "@rescript/core": "^1.1.0",
    "camelcase": "^8.0.0",
    "rescript": "^11.0.1",
    "rescript-nodejs": "^16.0.0"
  },
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "project:build": "yarn workspaces foreach -At run build",
    "project:clean": "yarn workspaces foreach -At run fmt"
  },
  "resolutions": {
    "@rescript-graphql-codegen/core-plus": "workspace:*",
    "@rescript-graphql-codegen/graphql": "workspace:*",
    "@rescript-graphql-codegen/graphql-codegen": "workspace:*",
    "@rescript-graphql-codegen/graphql-codegen-embed-lang": "workspace:*",
    "@rescript-graphql-codegen/near-re-operation-file": "workspace:*",
    "@rescript-graphql-codegen/base-types": "workspace:*",
    "@rescript-graphql-codegen/operations": "workspace:*"
  }
}
