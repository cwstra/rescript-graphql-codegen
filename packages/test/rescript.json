{
  "name": "@rescript-graphql-codegen/operations",
  "namespace": "graphql-codegen-operations",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".gitignored.mjs",
  "bsc-flags": [
    "-open RescriptGraphqlCodegenLib -open CorePlus"
