{
  "name": "@rescript-graphql-codegen/graphql-codegen",
  "version": "0.0.1",
  "keywords": [
    "rescript"
  ],
  "license": "MIT",
  "author": "cwstra",
  "scripts": {
    "build": "rescript",
    "graphql-codegen:build": "yarn build",
    "clean": "rescript clean",
    "graphql-codegen:clean": "yarn clean",
    "fmt": "rescript format",
    "graphql-codegen:fmt": "yarn fmt",
    "dev": "rescript build -w",
    "graphql-codegen:dev": "yarn workspaces foreach -R --from @rescript-graphql-codegen/graphql run build; yarn dev"
  },
  "dependencies": {
    "@rescript-graphql-codegen/core-plus": "workspace:*",
    "@rescript-graphql-codegen/graphql": "workspace:*",
    "rescript": "^11.0.1"
  },
  "peerDependencies": {
    "graphql": "^0.12.0 || ^0.13.0 || ^14.0.0 || ^16.0.0"
  }
}
